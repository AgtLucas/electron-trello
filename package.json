{
  "name": "treadstone",
  "productName": "Treadstone",
  "version": "1.0.0",
  "description": "Unofficial Trello app",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "npm run build-osx && npm run build-linux",
    "build-osx": "electron-packager . $npm_package_productName --overwrite --out=dist --ignore='^/dist$' --ignore='^/media$' --prune --platform=darwin --arch=x64 --icon=media/Icon.icns --app-version=$npm_package_version --version=$npm_package_electronVersion && cd dist/Treadstone-darwin-x64 && zip -ryXq9 ../Treadstone-${npm_package_version}.zip Treadstone.app",
    "build-linux": "electron-packager . $npm_package_productName --overwrite --out=dist --ignore='^/dist$' --ignore='^/media/(?!Icon.png$).*' --prune --platform=linux --arch=x64 --app-bundle-id=com.agtlucas.treadstone --app-version=$npm_package_version --version=$npm_package_electronVersion && cd dist/Treadstone-linux-x64/ && zip -ryq9 ../Treadstone-linux-\"$npm_package_version\".zip *"
  },
  "engines": {
    "node": ">=4"
  },
  "electronVersion": "0.33.8",
  "files": [
    "index.js",
    "media/icon.png"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AgtLucas/treadstone.git"
  },
  "author": "Lucas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AgtLucas/treadstone/issues"
  },
  "homepage": "https://github.com/AgtLucas/treadstone#readme",
  "dependencies": {
    "electron-debug": "^0.2.1"
  },
  "devDependencies": {
    "electron-packager": "^5.1.0",
    "electron-prebuilt": "^0.33.8"
  }
}